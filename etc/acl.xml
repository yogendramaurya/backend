<config xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:framework:Acl/etc/acl.xsd">
    <acl>
        <resources>
            <resource id="Magento_Backend::admin" >
                <resource id="Acodesh_Backend::Acodesh" title="Acodesh" sortOrder="200" >
                    <resource id="Acodesh_Backend::Acodesh_Documentation" title="Acodesh Documentation" sortOrder="0" />
                </resource>
                <resource id="Magento_Backend::stores">
                    <resource id="Magento_Backend::stores_settings">
                        <resource id="Magento_Config::config">
                            <resource id="Acodesh_Backend::DocumentationSettings" title="Acodesh DocumentationSettings" sortOrder="0" />
                        </resource>
                    </resource>
                </resource>
            </resource>
        </resources>
    </acl>
</config>